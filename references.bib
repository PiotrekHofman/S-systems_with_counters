@InProceedings{verbeek2000woflan,
  author    = {Eric Verbeek and Wil M. P. van der Aalst},
  booktitle = {Proc.\ $\text{21}^\text{st}$ Conference on Application and Theory of {P}etri Nets 2000, , ({ICATPN})},
  title     = {Woflan 2.0: {A} {P}etri-Net-Based Workflow Diagnosis Tool},
  year      = {2000},
  address   = {Berlin, Heidelberg},
  editor    = {Nielsen, Mogens and Simpson, Dan},
  pages     = {475--484},
  publisher = {Springer Berlin Heidelberg},
  abstract  = {Workflow management technology promises a flexible solution facilitating the easy creation of new business processes and modification of existing ones. Unfortunately, most of today's workflow products allow for erroneous processes to be put in production: these products lack proper verification mechanisms in their process-definition tools for the created or modified processes. This paper presents the workflow diagnosis tool Woflan, which fills this gap. Using Petri-net based techniques, Woflan diagnoses process definitions before they are put into production. These process definitions can be imported from commercial work-flow products. Furthermore, Woflan guides the modeler of a workflow process definition towards finding and correcting possible errors.},
  doi       = {10.1007/3-540-44988-4\_28},
  isbn      = {978-3-540-44988-1},
}



